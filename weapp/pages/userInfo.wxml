

<view class="container">

  <view class="userinfo">
    <image src="{{ userInfo.avatarUrl }}" background-size="cover" class="userinfo-avatar"></image>
    <view>
      <view class="userinfo-nickname">{{ userInfo.nickName }}</view>
      <view class="join">
        <image src="../assets/images/join.png"></image>  加盟合伙人 ></view>
    </view>
  </view>

  <view id="div1" class="userdiv">
    <view>
      <view class="num">{{userInfo.money}}</view>
      <view>钱包</view>
    </view>
    <view>
      <view class="num">{{collectNum}}</view>
      <view>收藏</view>
    </view>
  </view>
   
  <view class="userdiv">
    <view class="div1"><image src="../assets/images/star.png"></image>我的服务</view>
    <view class="div2">
      <view data-wpy-evt="8-5" bindtap="_proxy" class="div3">
        <image src="../assets/images/my1.png" style="background-color: #fba771"></image>
        <label>我的闲置</label>
      </view>
      <view class="div3">
        <image src="../assets/images/my2.png" style="background-color: #f88888"></image>
        <label>合伙人</label>
      </view>
      <view data-wpy-evt="8-6" bindtap="_proxy" class="div3">
        <image src="../assets/images/my3.png" style="background-color: #df764d"></image>
        <label>我的收藏</label>
      </view>
    </view>
  </view>

  <view class="userdiv">
    <view class="div1"><image src="../assets/images/star.png"></image>平台服务</view>
    <view class="div2">
      <view class="div3">
        <image src="../assets/images/pf1.png" style="background-color: #61ddc1"></image>
        <label>联系客服</label>
      </view>
      <view class="div3">
        <image src="../assets/images/pf2.png" style="background-color: #3dc3ec"></image>
        <label>帮助中心</label>
      </view>
      <view class="div3">
        <image src="../assets/images/pf3.png" style="background-color: #6798ef"></image>
        <label>关于我们</label>
      </view>
    </view>

  </view>


  <view class="userdiv">
    <view class="div1"><image src="../assets/images/star.png"></image>商家服务</view>
    <view class="div2">
      <view class="div3">
        <image src="../assets/images/store1.png" style="background-color: #97c9fd"></image>
        <label>我要入驻</label>
      </view>
      <view data-wpy-evt="8-7" bindtap="_proxy" class="div3">
        <image src="../assets/images/store2.png" style="background-color: #dca5f8"></image>
        <label>商家入口</label>
      </view>
       
    </view>
  </view>

   
 </view>
